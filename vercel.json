{
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/about",
            "destination": "/pages/about.html"
        },
        {
            "source": "/specialists",
            "destination": "/pages/specialists.html"
        },
        {
            "source": "/contacts",
            "destination": "/pages/contacts.html"
        },
        {
            "source": "/service/:service",
            "destination": "/pages/service.html?service=:service"
        },
        {
            "source": "/doctors/:id",
            "destination": "/pages/doctors.html?id=:id"
        }
    ],
    "redirects": [
        {
            "source": "/index.html",
            "destination": "/",
            "permanent": true
        },
        {
            "source": "/specialists.html",
            "destination": "/specialists",
            "permanent": true
        },
        {
            "source": "/contacts.html",
            "destination": "/contacts",
            "permanent": true
        },
        {
            "source": "/about.html",
            "destination": "/about",
            "permanent": true
        },
        {
            "source": "/service.html",
            "destination": "/service",
            "permanent": true
        },
        {
            "source": "/doctors.html",
            "destination": "/doctors",
            "permanent": true
        }
    ]
}