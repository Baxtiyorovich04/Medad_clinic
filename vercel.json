{
    "rewrites": [
        {
            "source": "/about",
            "destination": "/pages/about.html"
        },
        {
            "source": "/specialists",
            "destination": "/pages/specialists.html"
        },
        {
            "source": "/contacts",
            "destination": "/pages/contacts.html"
        },
        {
            "source": "/services/:service",
            "destination": "/pages/service.html?service=:service"
        },
        {
            "source": "/doctors/:id",
            "destination": "/pages/doctors.html?id=:id"
        }
    ],
    "redirects": [
        {
            "source": "/pages/(.*)",
            "destination": "/$1",
            "permanent": true
        },
        {
            "source": "/(.*).html",
            "destination": "/$1",
            "permanent": true
        }
    ]
}